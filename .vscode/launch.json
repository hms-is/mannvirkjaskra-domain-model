{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Build html",
            "type": "debugpy",
            "request": "launch",
            "preLaunchTask": "make html",
            "program": "${workspaceFolder}/pass.py",
            "console": "internalConsole"
        },
        {
            "name": "Clean html",
            "type": "debugpy",
            "request": "launch",
            "preLaunchTask": "make clean",
            "program": "${workspaceFolder}/pass.py",
            "console": "internalConsole"
        },
        {
            "name": "Open html",
            "type": "debugpy",
            "request": "launch",
            "preLaunchTask": "open html",
